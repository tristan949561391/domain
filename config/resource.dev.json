{
  "server": {
    "port": 80
  },
  "cookie": {
    "maxAge": 36000000,
    "httpOnly": true,
    "path": "/"
  },
  "session": {
    "key": "moodust",
    "prefix": "sess"
  },
  "redis": {
    "url": "localhost",
    "port": 6379
  },
  "mongo": {
    "url": "mongodb://localhost",
    "port": 27017,
    "db": "moondust"
  },
  "log4j": {
    "appenders": [
      {
        "type": "console",
        "category": "console"
      },
      {
        "type": "dateFile",
        "filename": "C:/data/",
        "pattern": "yyyyMMddhh.txt",
        "absolute": true,
        "alwaysIncludePattern": true,
        "category": "logInfo"
      }
    ],
    "levels": {
      "logInfo": "DEBUG"
    }
  }
}